{
  "name": "acromedia/drupal-orange-project",
  "description": "Provides a template to create a new Drupal 8 project via Composer.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "name": "drupal",
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "name": "orange_profile",
      "type": "vcs",
      "url": "https://github.com/AcroMedia/orange_profile"
    },
    {
      "name": "orange_ecom_profile",
      "type": "vcs",
      "url": "https://github.com/AcroMedia/orange_ecom_profile"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/colorbutton",
        "type": "drupal-library",
        "version": "4.7.3",
        "dist": {
          "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.7.3.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/font",
        "type": "drupal-library",
        "version": "4.7.3",
        "dist": {
          "url": "https://download.ckeditor.com/font/releases/font_4.7.3.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/panelbutton",
        "type": "drupal-library",
        "version": "4.7.3",
        "dist": {
          "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.7.3.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/ckeditor",
        "type": "drupal-library",
        "version": "4.5",
        "dist": {
          "url": "https://github.com/ckeditor/ckeditor-dev/archive/release/4.5.x.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "bgrins/spectrum",
        "type": "drupal-library",
        "version": "master",
        "source": {
          "url": "https://github.com/bgrins/spectrum",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "dimsemenov/magnific-popup",
        "type": "drupal-library",
        "version": "master",
        "source": {
          "url": "https://github.com/dimsemenov/Magnific-Popup",
          "type": "git",
          "reference": "origin/master"
        }
      }
    }
  ],
  "require": {
    "acromedia/orange_profile": "dev-8.x-1.x",
    "acromedia/orange_ecom_profile": "dev-8.x-1.x",
    "dimsemenov/magnific-popup": "master",
    "bgrins/spectrum": "master",
    "ckeditor/ckeditor": "4.5",
    "ckeditor/colorbutton": "4.7.3",
    "ckeditor/font": "4.7.3",
    "ckeditor/panelbutton": "4.7.3",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.5",
    "drupal/console": "^1.0.2",
    "drupal/core": "~8.5.3",
    "drush/drush": "^9.0.0",
    "ext-curl": "*",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "webflo/drupal-core-require-dev": "~8.5.3",
    "drupal/devel": "1.x-dev"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "initial": {
        ".editorconfig": "../.editorconfig",
        ".gitattributes": "../.gitattributes"
      }
    },
    "patches": {
      "drupal/core": {
        "drupalimage CKEditor plugin should not require data-entity-uuid and data-entity-type when image upload is disabled": "https://www.drupal.org/files/issues/drupalimage_ckeditor-2771837-17.patch"
      },
      "drupal/fivestar": {
        "No click operations on the star": "https://www.drupal.org/files/issues/uncaught_typeerror-2920834-2.patch",
        "Comments with ratings have clickable ratings for other users": "https://www.drupal.org/files/issues/comments_with_ratings-2919915-3.patch"
      }
    }
  }
}
